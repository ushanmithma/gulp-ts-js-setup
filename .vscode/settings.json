{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontFamily": "Hack",
  "editor.fontSize": 13,
  "editor.lineHeight": 1.45,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.45,
  "editor.tabSize": 2,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.colorCustomizations": {
    "focusBorder": "#86AFD0",
    "button.background": "#86AFD0",
    "button.foreground": "#191919",
    "button.hoverBackground": "#a0c0da",
    "textLink.foreground": "#86AFD0",
    "textLink.activeForeground": "#a0c0da",
    "activityBar.foreground": "#86AFD0",
    "activityBar.background": "#141414",
    "activityBarBadge.foreground": "#1f1f1f",
    "activityBarBadge.background": "#86afd0",
    "menu.background": "#191919",
    "menu.selectionBackground": "#292929",
    "menu.selectionBorder": "#292929",
    "menu.border": "#191919",
    "menu.separatorBackground": "#292929",
    "statusBar.foreground": "#a6a6a6",
    "statusBar.background": "#292929",
    "statusBarItem.remoteBackground": "#292929",
    "statusBarItem.remoteForeground": "#a6a6a6",
    "sideBar.background": "#191919",
    "list.activeSelectionBackground": "#86AFD04d",
    "list.focusAndSelectionOutline": "#86AFD04d",
    "list.focusOutline": "#2929294d",
    "list.hoverBackground": "#292929",
    "list.inactiveSelectionBackground": "#292929",
    "list.inactiveFocusOutline": "#292929",
    "list.inactiveFocusBackground": "#292929",
    "tab.activeBackground": "#1f1f1f",
    "tab.border": "#141414",
    "tab.inactiveBackground": "#141414",
    "breadcrumb.background": "#1f1f1f",
    "titleBar.activeBackground": "#141414",
    "titleBar.activeForeground": "#a6a6a6",
    "editor.background": "#1f1f1f",
    "editorGroupHeader.tabsBackground": "#141414",
    "editorGutter.background": "#1f1f1f"
  },
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "newOperator": "#b8b8ff",
      "stringLiteral": "#86AFD0",
      "customLiteral": "#56b3b4",
      "numberLiteral": "#F78C6C"
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["meta.embedded", "source.groovy.embedded"],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "scope": "emphasis",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "strong",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "header",
        "settings": {
          "foreground": "#000080"
        }
      },
      {
        "scope": "comment",
        "settings": {
          "foreground": "#697098"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": ["constant.numeric", "variable.other.enummember", "keyword.operator.plus.exponent", "keyword.operator.minus.exponent"],
        "settings": {
          "foreground": "#F78C6C"
        }
      },
      {
        "scope": "constant.regexp",
        "settings": {
          "foreground": "#646695"
        }
      },
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "entity.name.tag.css",
        "settings": {
          "foreground": "#f5d67b"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#BEC5D4"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name.class.css",
          "entity.other.attribute-name.class.mixin.css",
          "entity.other.attribute-name.id.css",
          "entity.other.attribute-name.parent-selector.css",
          "entity.other.attribute-name.pseudo-class.css",
          "entity.other.attribute-name.pseudo-element.css",
          "source.css.less entity.other.attribute-name.id",
          "entity.other.attribute-name.scss"
        ],
        "settings": {
          "foreground": "#f5d67b"
        }
      },
      {
        "scope": "invalid",
        "settings": {
          "foreground": "#f44747"
        }
      },
      {
        "scope": "markup.underline",
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.strikethrough",
        "settings": {
          "fontStyle": "strikethrough"
        }
      },
      {
        "scope": "markup.inserted",
        "settings": {
          "foreground": "#F78C6C"
        }
      },
      {
        "scope": "markup.deleted",
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "scope": "markup.changed",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "punctuation.definition.quote.begin.markdown",
        "settings": {
          "foreground": "#697098"
        }
      },
      {
        "scope": "punctuation.definition.list.begin.markdown",
        "settings": {
          "foreground": "#6796e6"
        }
      },
      {
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "name": "brackets of XML/HTML tags",
        "scope": "punctuation.definition.tag",
        "settings": {
          "foreground": "#808080"
        }
      },
      {
        "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "meta.preprocessor.string",
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "scope": "meta.preprocessor.numeric",
        "settings": {
          "foreground": "#F78C6C"
        }
      },
      {
        "scope": "meta.structure.dictionary.key.python",
        "settings": {
          "foreground": "#BEC5D4"
        }
      },
      {
        "scope": "meta.diff.header",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "storage",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "storage.type",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": ["storage.modifier", "keyword.operator.noexcept"],
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": ["string", "meta.embedded.assembly"],
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "scope": "string.tag",
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "scope": "string.value",
        "settings": {
          "foreground": "#C3E88D"
        }
      },
      {
        "scope": "string.regexp",
        "settings": {
          "foreground": "#d16969"
        }
      },
      {
        "name": "String interpolation",
        "scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end", "punctuation.section.embedded"],
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "name": "Reset JavaScript string interpolation expression",
        "scope": ["meta.template.expression"],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "scope": [
          "support.type.vendored.property-name",
          "support.type.property-name",
          "variable.css",
          "variable.scss",
          "variable.other.less",
          "source.coffee.embedded"
        ],
        "settings": {
          "foreground": "#BEC5D4"
        }
      },
      {
        "scope": "keyword",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "scope": [
          "keyword.operator.new",
          "keyword.operator.expression",
          "keyword.operator.cast",
          "keyword.operator.sizeof",
          "keyword.operator.alignof",
          "keyword.operator.typeid",
          "keyword.operator.alignas",
          "keyword.operator.instanceof",
          "keyword.operator.logical.python",
          "keyword.operator.wordlike"
        ],
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "keyword.other.unit",
        "settings": {
          "foreground": "#f5d67b"
        }
      },
      {
        "scope": ["punctuation.section.embedded.begin.php", "punctuation.section.embedded.end.php"],
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "scope": "support.function.git-rebase",
        "settings": {
          "foreground": "#BEC5D4"
        }
      },
      {
        "scope": "constant.sha.git-rebase",
        "settings": {
          "foreground": "#F78C6C"
        }
      },
      {
        "name": "coloring of the Java import and package identifiers",
        "scope": ["storage.modifier.import.java", "variable.language.wildcard.java", "storage.modifier.package.java"],
        "settings": {
          "foreground": "#89DDFF"
        }
      },
      {
        "name": "this.self",
        "scope": "variable.language",
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "name": "Function declarations",
        "scope": [
          "entity.name.function",
          "support.function",
          "support.constant.handlebars",
          "source.powershell variable.other.member",
          "entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
        ],
        "settings": {
          "foreground": "#56b3b4"
        }
      },
      {
        "name": "Types declaration and references",
        "scope": [
          "support.class",
          "support.type",
          "entity.name.type",
          "entity.name.namespace",
          "entity.other.attribute",
          "entity.name.scope-resolution",
          "entity.name.class",
          "storage.type.numeric.go",
          "storage.type.byte.go",
          "storage.type.boolean.go",
          "storage.type.string.go",
          "storage.type.uintptr.go",
          "storage.type.error.go",
          "storage.type.rune.go",
          "storage.type.cs",
          "storage.type.generic.cs",
          "storage.type.modifier.cs",
          "storage.type.variable.cs",
          "storage.type.annotation.java",
          "storage.type.generic.java",
          "storage.type.java",
          "storage.type.object.array.java",
          "storage.type.primitive.array.java",
          "storage.type.primitive.java",
          "storage.type.token.java",
          "storage.type.groovy",
          "storage.type.annotation.groovy",
          "storage.type.parameters.groovy",
          "storage.type.generic.groovy",
          "storage.type.object.array.groovy",
          "storage.type.primitive.array.groovy",
          "storage.type.primitive.groovy"
        ],
        "settings": {
          "foreground": "#f0c674"
        }
      },
      {
        "name": "Types declaration and references, TS grammar specific",
        "scope": [
          "meta.type.cast.expr",
          "meta.type.new.expr",
          "support.constant.math",
          "support.constant.dom",
          "support.constant.json",
          "entity.other.inherited-class"
        ],
        "settings": {
          "foreground": "#f0c674"
        }
      },
      {
        "name": "Control flow / Special keywords",
        "scope": [
          "keyword.control",
          "source.cpp keyword.operator.new",
          "keyword.operator.delete",
          "keyword.other.using",
          "keyword.other.operator",
          "entity.name.operator"
        ],
        "settings": {
          "foreground": "#b8b8ff"
        }
      },
      {
        "name": "Variable and parameter name",
        "scope": [
          "variable",
          "meta.definition.variable.name",
          "support.variable",
          "entity.name.variable",
          "constant.other.placeholder" // placeholders in strings
        ],
        "settings": {
          "foreground": "#BEC5D4"
        }
      },
      {
        "name": "Constants and enums",
        "scope": ["variable.other.constant", "variable.other.enummember"],
        "settings": {
          "foreground": "#f5a6ff"
        }
      },
      {
        "name": "Object keys, TS grammar specific",
        "scope": ["meta.object-literal.key"],
        "settings": {
          "foreground": "#BEC5D4"
        }
      },
      {
        "name": "CSS property value",
        "scope": [
          "support.constant.property-value",
          "support.constant.font-name",
          "support.constant.media-type",
          "support.constant.media",
          "constant.other.color.rgb-value",
          "constant.other.rgb-value",
          "support.constant.color",
          "meta.property-value"
        ],
        "settings": {
          "foreground": "#86AFD0"
        }
      },
      {
        "name": "Regular expression groups",
        "scope": [
          "punctuation.definition.group.regexp",
          "punctuation.definition.group.assertion.regexp",
          "punctuation.definition.character-class.regexp",
          "punctuation.character.set.begin.regexp",
          "punctuation.character.set.end.regexp",
          "keyword.operator.negation.regexp",
          "support.other.parenthesis.regexp"
        ],
        "settings": {
          "foreground": "#86AFD0"
        }
      },
      {
        "scope": [
          "constant.character.character-class.regexp",
          "constant.other.character-class.set.regexp",
          "constant.other.character-class.regexp",
          "constant.character.set.regexp"
        ],
        "settings": {
          "foreground": "#d16969"
        }
      },
      {
        "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
        "settings": {
          "foreground": "#56b3b4"
        }
      },
      {
        "scope": "keyword.operator.quantifier.regexp",
        "settings": {
          "foreground": "#f5d67b"
        }
      },
      {
        "scope": "constant.character",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": "constant.character.escape",
        "settings": {
          "foreground": "#f5d67b"
        }
      },
      {
        "scope": "entity.name.label",
        "settings": {
          "foreground": "#C8C8C8"
        }
      },
      // CSS SCSS Token Colors
      {
        "name": "CSS Property Name",
        "scope": ["support.type.property-name.css", "meta.property-name"],
        "settings": {
          "foreground": "#56b3b4"
        }
      },
      {
        "name": "CSS Numbers",
        "scope": ["constant.numeric.css"],
        "settings": {
          "foreground": "#fc9463"
        }
      },
      {
        "scope": "support.other.namespace",
        "settings": {
          "foreground": "#d4d4d4"
        }
      }
    ]
  }
}
